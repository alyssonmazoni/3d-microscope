issn = {19326203},
journal = {PLoS ONE},
month = {feb},
number = {2},
publisher = {Public Library of Science},
title = {{Openstage: A low-cost motorized microscope stage with sub-micron positioning accuracy}},
volume = {9},
year = {2014}
}
