keywords = {3-D microscope,imaging,microscope,open hardware,optics,stage},
month = {nov},
number = {2},
pages = {238--246},
publisher = {Blackwell Publishing Ltd},
title = {{Free and open-source automated 3-D microscope}},
volume = {264},
year = {2016}
}
