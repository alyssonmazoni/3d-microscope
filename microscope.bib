issn = {0003-6935},
journal = {Applied Optics},
month = {mar},
number = {9},
pages = {D127},
publisher = {The Optical Society},
title = {{Compact and field-portable 3D printed shearing digital holographic microscope for automated cell identification}},
volume = {56},
year = {2017}
