@article{Falkingham2012,
abstract = {The 3D digitisation of palaeontological resources is of tremendous use to the field, providing the means to archive, analyse, and visualise specimens that would otherwise be too large to handle, too valuable to destructively sample, or simply in a different geographic location. Digitisation of a specimen to produce a 3D digital model often requires the use of expensive laser scanning equipment or proprietary digital reconstruction software, making the technique inaccessible to many workers. Presented here is a guide for producing high resolution 3D models from photographs, using freely available open-source software. To demonstrate the accuracy and flexibility of the approach, a number of examples are given, including a small trilobite ({\~{}}0.04 m), a large mounted elephant skeleton ({\~{}}3 m), and a very large fossil tree root system ({\~{}}6 m), illustrating that the method is equally applicable to specimens or even outcrops of all sizes. The digital files of the models produced in this paper are included. The results demonstrate that production of digital models from specimens for research or archival purposes is available to anyone, and it is hoped that an increased use of digitisation techniques will facilitate research and encourage collaboration and dissemination of digital data. {\textcopyright} Palaeontological Association February 2012.},
author = {Falkingham, Peter L.},
doi = {10.26879/264},
file = {:C$\backslash$:/Users/Leandro/Downloads/264.pdf:pdf},
issn = {10948074},
journal = {Palaeontologia Electronica},
keywords = {Archive,Digital,Fossil,Laser scanning,Model,Photogrammetry},
number = {1},
title = {{Acquisition of high resolution three-dimensional models using free, open-source, photogrammetric software}},
volume = {15},
year = {2012}
